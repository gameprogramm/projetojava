public class Fruto {
    private String nome;
    private boolean comestivel;

    public String getNome() {
        return nome;
    }

    public void setNome(String nome) {
        this.nome = nome;
    }

    public boolean isComestivel() {
        return comestivel;
    }

    public void setComestivel(boolean comestivel) {
        this.comestivel = comestivel;
    }
}
